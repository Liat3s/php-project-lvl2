#!/usr/bin/env php
<?php
$doc = <<<DOC

Generate diff

Usage:
    gendiff (-h|--help)
    gendiff (-v|--version)

Options:
    -h --help           Show this screen
    -v --version        Show version

DOC;

require_once __DIR__ . '/../vendor/autoload.php';

use \Docopt as Docopt;
$args = Docopt::handle($doc, array('version'=> 'Generate diff 0.1'));
foreach ($args as $k=>$v)
    echo $k.': '.json_encode($v).PHP_EOL;
